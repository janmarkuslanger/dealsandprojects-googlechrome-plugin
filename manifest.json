{
  "name": "Deals and Projects - Optimizations",
  "version": "0.0.1",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "inject.js"
      ],
      "run_at": "document_end",
      "all_frames": false
    }
  ]
}
